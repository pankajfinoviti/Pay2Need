<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#fcfbfb"
    android:id="@+id/parentLayout"
    tools:context=".aeps.IciciKotakAllTxnReceiptActivity">

    <include
        android:id="@+id/titlebar"
        layout="@layout/titlebar_share"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <ScrollView
        android:id="@+id/screen"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">



            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="16dp">

                <LinearLayout
                    android:id="@+id/zigzagtop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/zigzag_line"
                    android:orientation="vertical" />

                <LinearLayout
                    android:id="@+id/detailLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/zigzagtop"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/StatusLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <ImageView
                                    android:id="@+id/icon_bankLogo"
                                    android:layout_width="130dp"
                                    android:layout_height="30dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/icici_logo" />

                                <ImageView
                                    android:id="@+id/iv_printer"
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:layout_alignParentEnd="true"
                                    android:padding="15dp"
                                    android:src="@drawable/icon_printer" />
                            </RelativeLayout>

                            <ImageView
                                android:id="@+id/icon_status"
                                android:layout_width="45dp"
                                android:layout_height="45dp"
                                android:layout_marginTop="10dp"
                                android:src="@drawable/hotel_booked_success" />

                        </LinearLayout>

                        <TextView
                            android:id="@+id/status"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:gravity="center"
                            android:text="Successful"
                            android:textColor="@color/colorPrimary1"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/bitBlack"
                        android:visibility="gone" />

                    <LinearLayout
                        android:id="@+id/ll_hideShow_txnId"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Bank Name"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_bankName"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="end"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/bitBlack" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_hideShow_bcName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="BC Name"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_bcName"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="right"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/bitBlack" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal"
                            android:weightSum="2">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight=".8"
                                android:text="BC Id"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_bcId"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.2"
                                android:gravity="right"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/bitBlack" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal"
                            android:weightSum="2">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight=".8"
                                android:text="BC Location"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_bcLocation"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.2"
                                android:gravity="right"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/bitBlack" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_hideShow_fee"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:visibility="gone"
                            android:id="@+id/linear_terminal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Terminal Id"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_terminalId"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="right"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/lienar_bene"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Bene Name"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_BeneName"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="right"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/bitBlack" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_hideShow_custNo"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Customer Number"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_custNo"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="right"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/bitBlack" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_hideShow_kotak"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Customer Name"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textCustomerName"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="end"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="8dp"
                            android:background="@color/bitBlack" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Reference Id"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textReferenceId"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="end"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="8dp"
                            android:background="@color/bitBlack" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/linear_aadhaar"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Aadhaar Number"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textAadharNumber"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="end"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <LinearLayout
                            android:visibility="gone"
                            android:id="@+id/linear_account"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Account No"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textAadharNumber1"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="end"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/bitBlack" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="RRN"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_rrn"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="end"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/bitBlack" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_hideShow_stan"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_billerDetails"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Stan Number"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_stanNo"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="end"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/bitBlack" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_hideShow_txnDate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal"
                            android:weightSum="2">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight=".8"
                                android:text="Date Time"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_txnDate"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.2"
                                android:gravity="end"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/bitBlack" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_hideShow_txnAmt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/balance_dynamic"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Txn Amount"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/icici_balance"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.5"
                                android:gravity="end"
                                android:textColor="@color/colorPrimary1"
                                android:textSize="@dimen/last_five_detail_text" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="20dp"
                        android:orientation="horizontal"
                        android:padding="@dimen/aeps_detail_padding">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Remarks :"
                            android:textColor="@color/black1"
                            android:textSize="@dimen/aeps_detail_text_size" />

                        <TextView
                            android:id="@+id/icici_remark"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="4dp"
                            android:textSize="@dimen/aeps_detail_text_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        android:padding="@dimen/aeps_detail_padding">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Helpline - "
                            android:textSize="8sp" />

                        <TextView
                            android:id="@+id/tv_bcEmailid"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="8sp" />

                        <TextView
                            android:id="@+id/tv_bcMobileNo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="8sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        android:padding="@dimen/aeps_detail_padding">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Consent - "
                            android:textColor="@color/darkTimeout"
                            android:textSize="8sp" />

                        <TextView
                            android:id="@+id/aepsConsentText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="4dp"
                            android:text="@string/icici_consentString"
                            android:textColor="@color/darkTimeout"
                            android:textSize="7sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/hideShowBranding"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20dp"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/dotted_line"
                            android:layerType="software" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_weight=".7"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Customer Care"
                                    android:textColor="@color/black1"
                                    android:textSize="8sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Email : ccare@bankmitra.org"
